{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "inventory": {
      "type": "application",
      "root": "apps/inventory",
      "entryFile": "main",
      "sourceRoot": "apps/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory/tsconfig.app.json"
      }
    },
    "oms": {
      "type": "application",
      "root": "apps/oms",
      "entryFile": "main",
      "sourceRoot": "apps/oms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/oms/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "Paymentmain",
      "sourceRoot": "apps/payment",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "wms": {
      "type": "application",
      "root": "apps/wms",
      "entryFile": "main",
      "sourceRoot": "apps/wms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/wms/tsconfig.app.json"
      }
    },
    "log": {
      "type": "application",
      "root": "apps/log",
      "entryFile": "logmain",
      "sourceRoot": "apps/log",
      "compilerOptions": {
        "tsConfigPath": "apps/log/tsconfig.app.json"
      }
    }
  }
}